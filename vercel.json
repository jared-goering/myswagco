{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "functions": {
    "app/api/garments/import-from-url-smart/route.ts": {
      "maxDuration": 30,
      "memory": 1024
    },
    "app/api/garments/import-from-url/route.ts": {
      "maxDuration": 15,
      "memory": 512
    },
    "app/api/artwork/generate/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "app/api/artwork/vectorize/route.ts": {
      "maxDuration": 30,
      "memory": 512
    },
    "app/api/artwork/remove-background/route.ts": {
      "maxDuration": 30,
      "memory": 512
    },
    "app/api/payments/webhook/route.ts": {
      "maxDuration": 30,
      "memory": 256
    }
  },
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, must-revalidate" }
      ]
    }
  ]
}

